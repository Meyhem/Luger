{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Jwt": {
    "Audience": "Luger",
    "Issuer": "Luger",
    "IncludeErrorDetails": true,
    "TokenValidationParameters": {
      "IssuerSigningKey": "your-signing-key-here",
      "ValidIssuer": "Luger",
      "ValidAudience": "Luger",
      "RequireExpirationTime": true
    }
  },
  "Luger": {
    "Users": [
      {
        "Id": "admin",
        "Buckets": [ "bucket1" ]
      }
    ],
    "Buckets": [
      { "Name": "bucket1" }
    ]
  },
  "AllowedHosts": "*"
}
